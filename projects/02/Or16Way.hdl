/**
 * 16-way Or: 
 * out = (in[0] or in[1] or ... or in[15])
 */


CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    
    Or8Way(in[0..7]=in[0..7], out=out1);
    Or8Way(in[0..7]=in[8..15], out=out2);
    Or(a=out1, b=out2, out=out);

}